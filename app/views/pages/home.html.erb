<div class="homepage">
  <div class="banner" style ="background-image: url('https://plus.unsplash.com/premium_photo-1676422353822-acd056e0bdce?q=80&w=1967&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D');">
    <div class="container">
      <div class="head">
        <h1>AIRPNP</h1>
        <p>everything you need for a party!</p>
      </div>
    </div>
  </div>

  <div class="items">
    <h2 id="popular">
      Most popular this week
    </h2>

    <div class="container d-flex justify-content-between">
        <%# Card 1 %>
        <% @items.each do |item| %>
          <div class="card">

            <h2 class="card-title">
             <%= item.name %>
            </h2>
            <p>
             <%= item.price %>
            </p>
            <p>
             <%= item.description %>
            </p>
          </div>
        <% end %>
      <%= link_to "All listings", items_path %>
    </div>

  </div>



</div>
